       EO_Attendance.OFF_DAY_WORKING, 
       EO_Attendance.POLICY_EXCEPTION, 
       EO_Attendance.POLICY_EXCEPTION_APPROVED_FLAG, 
       EO_Attendance.START_TIME, 
       EO_Attendance.TYPE_OF_LEAVE,
       EO_Irregularities.EXCEPTION_APPROVED, 
       EO_Irregularities.EXCEPTION_REMARKS, 
       EO_Irregularities.IRREGULARITY_ID, 
       EO_Irregularities.LEAVE_TYPE, 
       EO_Irregularities.MINUTES_MISSING, 
       EO_Irregularities.TYPE, 
       EO_ConsumedLeaves.CAUSE, 
       EO_ConsumedLeaves.CONSUMED_LEAVES_ID, 
       EO_ConsumedLeaves.JOB_STATUS, 
       EO_ConsumedLeaves.LEAVE_DATE
FROM XX_E_PORTAL_EMP_ATD EO_Attendance, XX_E_PORTAL_EMP_IRREGULARITIES EO_Irregularities, XX_E_PORTAL_CONSUMED_LEAVES EO_ConsumedLeaves
where EO_ATTENDANCE.EMP_ID = EO_IRREGULARITIES.EMP_ID
and EO_ATTENDANCE.EMP_ATD_ID = EO_IRREGULARITIES.EMP_ATD_ID
and EO_IRREGULARITIES.EMP_ID = EO_ConsumedLeaves.USER_ID(+)
and EO_IRREGULARITIES.ATTENDANCE_DATE = EO_CONSUMEDLEAVES.LEAVE_DATE(+)
and EO_IRREGULARITIES.ATTENDANCE_DATE between :startDate and :endDate]]>
